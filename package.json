{
  "name": "dentist-theme",
  "version": "1.0.0",
  "description": "wordpress theme made for dentist clinics projects",
  "main": "assets/build/js/index.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --progress",
    "dev": "cross-env NODE_ENV=development webpack --watch --mode development --progress",
    "clean": "rm -rf assets/build/*",
    "svg": "svgr -d assets/src/js/icons assets/src/icons",
    "lint": "eslint assets/src/js/*",
    "lint:fix": "npm run lint --fix",
    "lint-css": "stylelint assets/src/scss/*.scss",
    "lint-css:fix": "npm run lint-css -- --fix",
    "build": "npm run lint-css:fix && npm run lint:fix && npm run prod"
  },
  "keywords": [
    "wordpress",
    "themes"
  ],
  "author": "Sarah Siqueira",
  "license": "GPLv2",
  "private": true,
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@wordpress/dependency-extraction-webpack-plugin": "^4.18.0",
    "@wordpress/eslint-plugin": "^14.8.0",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "cssnano": "^6.0.1",
    "eslint": "^7.32.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jsdoc": "^46.2.6",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.7.6",
    "node-sass": "^9.0.0",
    "postcss-scss": "^4.0.6",
    "sass-loader": "^13.3.2",
    "sass-mq": "^6.0.0",
    "style-loader": "^3.3.3",
    "stylelint": "^15.10.1",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-config-wordpress": "^17.0.0",
    "stylelint-webpack-plugin": "^4.1.1",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4"
  }
}
